{
  "name": "ControlAula button launcher",
  "version": "0.1",
  "description": "ControlAula button launcher",
  "background_page": "background.html",
  "icons": { "controlaula": "controlaula-button.png", "weblauncher" : "controlaula-button2.png" },
  "browser_action": {
    "default_icon": "controlaula-button.png",
    "default_title": "ControlAula",
    "name": "Click to see your clasroom with ControlAula"
  },
  "permissions": [
    "tabs", "file:///*/*/controlaula.html", "http://localhost:*/*"
  ]
}


